{
  "name": "espo-ui",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --port 8080",
    "backend:docker": "cd backend && docker-compose up -d",
    "dev:docker": "npm run backend:docker && docker-compose up -d",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test:vitest": "vitest",
    "test:jest": "jest",
    "lint:eslint": "eslint \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:stylelint": "stylelint --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "commit": "cz"
  },
  "dependencies": {
    "axios": "^0.25.0",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.1",
    "naive-ui": "^2.25.1",
    "pinia": "^2.0.11",
    "vis-timeline": "^7.5.0",
    "vue": "^3.2.30",
    "vue-router": "^4.0.12",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@iconify-json/mdi": "^1.0.17",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/vue": "^6.5.1",
    "@types/jest": "^27.4.0",
    "@types/lodash-es": "^4.17.6",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "@vitejs/plugin-legacy": "^1.7.1",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vue/compiler-sfc": "^3.2.30",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vue/vue3-jest": "^27.0.0-alpha.4",
    "autoprefixer": "^10.4.2",
    "commitizen": "^4.2.4",
    "commitlint": "^16.2.1",
    "cssnano": "^5.0.17",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.13.4",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-define-config": "^1.2.4",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-testing-library": "^5.0.5",
    "eslint-plugin-vue": "^8.4.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "jest-watch-typeahead": "^1.0.0",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.3.3",
    "postcss": "^8.4.6",
    "postcss-import": "^14.0.2",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-visualizer": "^5.5.4",
    "stylelint": "^14.4.0",
    "stylelint-config-recommended-vue": "^1.3.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-tailwindcss": "^0.0.3",
    "tailwindcss": "^3.0.21",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5",
    "unplugin-icons": "0.12.20",
    "unplugin-vue-components": "^0.17.18",
    "vite": "^2.8.0",
    "vite-plugin-cdn-import": "^0.3.5",
    "vite-plugin-compress": "^2.1.0",
    "vite-plugin-html": "^2.0.7",
    "vite-plugin-style-import": "^1.0.1",
    "vitest": "^0.3.5",
    "vue-eslint-parser": "^8.2.0",
    "vue-tsc": "^0.29.8",
    "webpack": "^5.68.0"
  },
  "lint-staged": {
    "*.{vue,js,ts,tsx}": "eslint --fix",
    "*.js": "eslint --cache --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": true
}
